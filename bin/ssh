#! /bin/sh
plugins="Status,Socket,SSH,Terminal"
cdir=`dirname $0`/..
if [ -r $cdir/jar/cryptix.jar ]; then
  echo "You need to download the cryptix.jar file and place it into"
  echo "the jar directory of the Distribution to use this script!"
  exit
fi
java -classpath $CLASSPATH:$cdir:$cdir/jar/cryptix.jar \
  de.mud.jta.Main -plugins $plugins $* 22
